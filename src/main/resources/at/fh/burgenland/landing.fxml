<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="400.0" spacing="20.0"
  xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="at.fh.burgenland.LandingController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>

  <fx:include fx:id="inputControlsPane" source="input_controls.fxml"/>
  <fx:include fx:id="profilAuswahlPane" source="profile.fxml"/>

  <Label fx:id="welcomeText"/>
  <Button onAction="#onHelloButtonClick" text="Hello!"/>

  <Button layoutX="44.0" layoutY="177.0" mnemonicParsing="false" onAction="#switchToDebug"
    text="DebugMenu">
    <font></font>
  </Button>

  <Button onAction="#showCoordinateSystem" text="Show Coordinate System"/>

</VBox>